						android:padding="8dp"
						android:background="#FFCCBC"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview8"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="MARATHI MEDIUM"
